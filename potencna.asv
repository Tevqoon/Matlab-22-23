function [y2, r2] = potencna(A, z0, koraki, eps)
    y = zeros(koraki);
    r = zeros(koraki);
    y(2) = A * z0;
    r(1) = z0' * y1;
    for k = 1:koraki
       if norm(y(k+1) - r(k) * z(k)) >= eps
           y2 = y(k+1)
           break
       end
       z(k) = y(k) / norm(y(k));
       z(k + 1) = A * z(k);
       r(k)
    end
end